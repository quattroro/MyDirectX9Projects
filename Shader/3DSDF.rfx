    <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_3_0" ENTRY_POINT="ps_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[

#define MAX_STEPS 100
#define MAX_DIST 100.0
#define SURF_DIST 0.01

struct VS_OUTPUT
{
   float4 Position : POSITION0;
   float2 TexCoord : TEXCOORD0;
};

struct Plane
{
   float3 normal;
   float height;
   float3 color;
};

struct Sphere
{
   float3 pos;
   float radius;
   float3 color;
};

struct Box
{
   float3 pos;
   float3 size;
   float3 color;
};

struct Vesica
{
   float3 pos;
   float r;
   float d;
};

float fSinTime0_X;
float fCosTime0_X;

float3 CameraPos;
float3 CameraRot;

float3 PlanePos;
float3 BoxPos;
float3 SpherePos;
float3 SpherePos2;
float3 VesicaPos;

float Rounding;
float Smoothing;
float Repeat;

float opUnion(float d1, float d2)
{
   return min(d1, d2);
}

float opSubtraction(float d1, float d2)
{
   return max(-d1, d2);
}

float opIntersection(float d1, float d2)
{
   return max(d1, d2);
}

float opXor(float d1, float d2)
{
   return max(min(d1, d2), -max(d1, d2));
}

float SDRounding(float p, float d)
{
   return p - d;
}

float opSmoothUnion(float d1, float d2, float k)
{
   float h = clamp(0.5 + 0.5 * (d1 - d1) / k, 0.0, 1.0);
   return lerp(d2, d1, h) - k * h * (1.0 - h);
}

float opSmoothSubtraction(float d1, float d2, float k)
{
   float h = clamp(0.5 - 0.5 * (d2 + d1) / k, 0.0, 1.0);
   return lerp(d2, -d1, h) + k * h * (1.0 - h);
}

float opSmoothIntersection(float d1, float d2, float k)
{
   float h = clamp(0.5 - 0.5 * (d2 - d1) / k, 0.0, 1.0);
   return lerp(d2, d1, h) + k * h * (1.0 - h);
}



float pSD(float3 p, Plane o)
{
   return dot(p, normalize(o.normal)) - o.height;
}

float sSD(float3 p, Sphere o)
{
   return length(p - o.pos) - o.radius;
}


float bSD(float3 p, Box o)
{
   float3 q = abs(p - o.pos) - o.size;
   return length(max(q, 0)) + min(max(q.x, max(q.y, q.z)), 0.0);
}

float opExtrusion( in float3 p, in float sdf, in float h )
{
   float2 w = float2( sdf, abs(p.z) - h );
   return min(max(w.x,w.y),0.0) + length(max(w,0.0));
}

float2 opRevolution( in float3 p, float w )
{
    return float2( length(p.xz) - w, p.y );
}

float sdVesica(float2 p, Vesica o)
{
   float2 q = abs(p - o.pos.xy);
   float b = sqrt(o.r * o.r - o.d * o.d);
   return ((q.y - b)*o.d > q.x * b) ? length(q - float2(0.0, b)) : length(q - float2(-o.d, 0.0))- o.r;
}

float repeated(float3 p, float s, Box o)
{
   float3 r = p - s * round(p/s);
   return bSD(r, o);
}

float onion( in float d, in float h )
{
    return abs(d)-h;
}



float GetDist(float3 p)
{
   Plane pl;
   pl.normal = float3(0.0, 1.0, 0.0);
   pl.height = 0.0;
   
   Sphere s;
   s.pos = SpherePos;
   s.radius = 1.0;
   
   Box b;
   b.pos = BoxPos;
   b.size = float3(1.0, 1.0, 0.5);
   
   Vesica v;
   v.pos = VesicaPos;
   v.d = 0.1;
   v.r = 0.5;
   
   
   Vesica v2;
   v2.pos = VesicaPos;
   v2.pos.z -= 1.2;
   v2.d = 0.1;
   v2.r = 0.5;
   
   
   Sphere s2;
   s2.pos = SpherePos2;
   s2.radius = 1.0;
   
   float dPlane = pSD(p, pl);
   float dSphere = sSD(p - s.pos, s);
   float dBox = SDRounding(bSD(p - b.pos, b), Rounding);
   float dSphere2 = onion(sSD(p - s2.pos, s2), 0.03);
   
   float dvesica = sdVesica(p - v.pos, v);
   dvesica = opExtrusion(p - v.pos, sdVesica(p - v.pos, v), 0.005);
   
   
   float dvesica2 = sdVesica(opRevolution(p - v2.pos, 0.0), v2 );
   
   //float dBox = SDRounding(repeated(p,Repeat, b), Rounding);
   
   
   //float d = min(dPlane, dSphere);
   //d = min(d, dBox);
   
   
   float d = min(dPlane, opSmoothSubtraction(dSphere, dBox, Smoothing));
   d = min(d, dvesica);
   d = min(d, dvesica2);
   d = min(d, dSphere2);
   return d;


//   float4 s = float4(4,3,8,2);
//   float sphereDist = length(p - s.xyz) - s.w;
//   float planeDist = p.y;
//   float d = min(sphereDist, planeDist);
//   return d;
}

float RayMarch(float3 ro, float3 rd)
{
   float d0 = 0.0;
   for(int i = 0; i < MAX_STEPS; i++)
   {
      float3 p = ro + rd * d0;
      float ds = GetDist(p);
      d0 += ds;
      if(d0 > MAX_DIST || ds < SURF_DIST)
         break;
   }
   
   return d0;
}

float3 GetNormal(float3 p)
{
   float d = GetDist(p);
   float2 e = float2(0.01, 0);
   float3 n = d - float3(
                  GetDist(p - e.xyy),
                  GetDist(p - e.yxy),
                  GetDist(p - e.yyx));
                  
   return normalize(n);
}



float GetLight(float3 p)
{
   float3 lightPos = float3(5,19,8);
   lightPos.xz += float2(fSinTime0_X * 19.0,fCosTime0_X * 19.0);
   float3 l = normalize(lightPos - p);
   float3 n = GetNormal(p);
   
   float dif = clamp(dot(n, l), 0.0, 1.0);
   float d = RayMarch(p + n * SURF_DIST * 2.0, l);
   if(d < length(lightPos - p))
      dif *= 0.1;
      
   return dif;
}



float4 ps_main(VS_OUTPUT In) : COLOR0
{   
   float3 col = float3(0,0,0);
   float2 uv = float2(In.TexCoord.x/1.0, In.TexCoord.y/1.0);
   uv = (uv - 0.5) * float2(1.0/1.0,1.0) * float2(2.0, 2.0);
   uv /= 0.3;
   float3 ro = float3(CameraPos.x,CameraPos.y,CameraPos.z);
   float3 rd = normalize(float3(uv.x  - CameraRot.x, -uv.y  - CameraRot.y, 1.0));
   //float3 rd = normalize(float3(uv.x, -uv.y, 1));
   float d = RayMarch(ro, rd);
   float3 p = ro + rd * d;
   float dif = GetLight(p);
   col = float3(dif, dif, dif);
   col = pow(col, float3(0.4545,0.4545,0.4545));
   
   return float4(col, 1.0);
   
   //return( float4( 1.0f, 1.0f, 0.0f, 1.0f ) ); 
}



]]></RmShaderCode>
