     <RmShaderConstant NAME="Smoothing" API="" REGISTER="9"/>
     <RmShaderConstant NAME="SpherePos" API="" REGISTER="5"/>
     <RmShaderConstant NAME="SpherePos2" API="" REGISTER="6"/>
     <RmShaderConstant NAME="VesicaPos" API="" REGISTER="7"/>
     <RmShaderConstant NAME="fCosTime0_X" API="" REGISTER="1"/>
     <RmShaderConstant NAME="fSinTime0_X" API="" REGISTER="0"/>
    </RmHLSLShader>
