     <RmShaderConstant NAME="g_CloudColor" API="" REGISTER="9"/>
     <RmShaderConstant NAME="g_LightColor" API="" REGISTER="10"/>
     <RmShaderConstant NAME="g_absorption" API="" REGISTER="5"/>
     <RmShaderConstant NAME="g_camPos" API="" REGISTER="6"/>
     <RmShaderConstant NAME="g_targetDepth" API="" REGISTER="4"/>
     <RmShaderConstant NAME="g_targetPos" API="" REGISTER="7"/>
