    <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_2_a" ENTRY_POINT="ps_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[

struct VS_OUTPUT 
{
   float4 Position : POSITION0;
   float2 TextureUV : TEXCOORD0;
   float2 BlendTextureUV : TEXCOORD1;
};


sampler2D UITextureSampler;
//sampler2D BlendUITextureSampler;

float _mtrlDif;
float4 _Color;
//float _OutlineWidth;
//float3 _MainTex_TexelSize;

float _Noise_Amount;
float _radian;
float _tempfloat;
float _xPos;
float _yPos;
float2 _resolution;

   
   float       _OutlineWidth; 
   float3      _MainTex_TexelSize = {0.00097, 0.00130,0.0};
   float4      _OutlineColor;
   
   
   
   
float4 Generate_Noise(float2 co, float black)
{
     float4 r = frac(sin(dot(co.xy, float2(12.9898, 78.233))) * 43758.5453);
     r.a = saturate(r.a + black);
     return r;
}

float2 hash( float2 p ) 
{
   p = float2( dot(p,float2(127.1,311.7)),
           dot(p,float2(269.5,183.3)) );

   return -1.0 + 2.0*frac(sin(p) * 43758.5453123);
}


float noise_2( in float2 p ) {
    const float K1 = 0.366025404; // (sqrt(3)-1)/2;
    const float K2 = 0.211324865; // (3-sqrt(3))/6;

   float2 i = floor( p + (p.x+p.y) * K1 );

    float2 a = p - i + (i.x+i.y) * K2;
    float2 o = step(a.yx,a.xy);
    float2 b = a - o + K2;
   float2 c = a - 1.0 + 2.0*K2;

    float3 h = max( 0.5-float3(dot(a,a), dot(b,b), dot(c,c) ), 0.0 );

   float3 n = h*h*h*h*float3( dot(a,hash(i+0.0)), dot(b,hash(i+o)), dot(c,hash(i+1.0)));

    return dot( n, float3(70.0,70.0,70.0) );
}

float4 fbm ( in float2 p ) {
    float f = 0.0;
    float2x2 m = { cos(_radian), sin(_radian),-sin(_radian), cos(_radian) };
    
    f  = (0.5000 * noise_2(p)).x; 
    p = mul(p,m);
    f += (0.2500 * noise_2(p)).x; 
    p = mul(p,m);
    f += (0.1250 * noise_2(p)).x; 
    p = mul(p,m);
    f += (0.0625 * noise_2(p)).x; 
    p = mul(p,m);
    f = 0.5 + 0.5 * f;
    float4 result = float4(f,f,f,f);
    return result;
}

float4 rndpixel(float2 p)
{
   float2 ipos = floor(p);
   float2 fpos = frac(p);
   
   float4 color = Generate_Noise(ipos,0);
   color.a = smoothstep(0+_tempfloat,1,color.a);
   //color.w = 1.0f;
   return color;

}

float rnd(float x)
{
   return frac(sin(x) * 10000);
}

float rnd(float2 st)
{
   return frac(sin(dot(st.xy, float2(12.9898,78.233)))* 43758.5453123);
}

float pattern(float2 st, float2 v, float t)
{
   float2 p = floor(st + v);
   return step(t,rnd(100.0 + p * 0.000001) + rnd(p.x)*0.5);

}

float4 ps_main(VS_OUTPUT In):COLOR0
{   

   //float4 tempUv = rndpixel(In.TextureUV * 50 + float2(_xPos,_yPos));
      
   
   //In.TextureUV = lerp(In.TextureUV, float2(tempUv.x,0.5), _Noise_Amount);
   
   In.TextureUV = In.TextureUV/_resolution;
   In.TextureUV.x *= _resolution.x/_resolution.y;
   
   float2 grid = float2(60.0,50.0);
   
   In.TextureUV *= grid;
   
   
   //float4 textureColor = tex2D(UITextureSampler, In.TextureUV);//메인 텍스쳐
   float4 textureColor = float4(1,1,1,1);
   float originAlpha = textureColor.a; 
    
 
   float2 ipos = floor(In.TextureUV);
   float2 fpos = frac(In.TextureUV);
   
   float2 offset = float2(0.1, 0);
   float2 vel = float2(_radian,0.0);
   textureColor.x = pattern(In.TextureUV + offset,vel, 0.5 + _tempfloat );
   textureColor.y = pattern(In.TextureUV,vel, 0.5+ _tempfloat);
   textureColor.z = pattern(In.TextureUV - offset,vel, 0.5+ _tempfloat);
   
   textureColor *= step(0.2,fpos.y);
   
   
   textureColor *= _Color;
     
   //return float4(1.0 - textureColor.xyz, 1.0);
   return textureColor;
   
}]]></RmShaderCode>
