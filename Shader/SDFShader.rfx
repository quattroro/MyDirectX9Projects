      //{
      //   float t = d / _BorderWidth;
      //   t = t * t;
      //   color = lerp(_borderColor, _baseColor, t);
      //}
      
      if(d<=0)
      {
         color.a = 0;
      }
      
   
   }
   else if(effect <8 )//7
   {
      //Neon
      float d = tex2D(_FontSampler, In.TextureUV).a + _Offset;
      color = float4(0,0,0,0);

      //only do something if within range of border
      if (d > -_BorderWidth && d < _BorderWidth)          
      {                  
      //calculate a value of 't' that goes from 0->1->0
      //around the edge of the geometry
      float t = d / _BorderWidth; //[-1:0:1]
      t = 1 - abs(t);             //[0:1:0]

      //lerp between background and border using t
      color = lerp(float4(0,0,0,0), _borderColor, t);

      //raise t to a high power and add in as white
      //to give bloom effect
      color.rgb += pow(t, _NeonPower)*_NeonBrightness;
      }
      
      //color = float4(d,d,d,1);
      
   }
   else if(effect < 9 )//8
   {
      //DropShadow
      float d = tex2D(_FontSampler, In.TextureUV).a + _Offset;
      color = float4(0,0,0,0);

      //take another sample, _ShadowDist texels up/right from the first
      float d2 = tex2D(_FontSampler, In.TextureUV + _ShadowDist * _MainTex_TexelSize.xy).a + _Offset;

      //calculate interpolators (go from 0 to 1 across border)
      //float fill_t =  saturate((d-_BorderWidth)/_BorderWidth);
      //float shadow_t =  saturate((d2-_ShadowBorderWidth)/_ShadowBorderWidth);
      
      float fill_t =  saturate((d)/_BorderWidth);
      float shadow_t =  saturate((d2)/_ShadowBorderWidth);

      //apply the shadow colour, then over the top apply fill colour
      color = lerp(color,_borderColor,shadow_t);
      color = lerp(color,_baseColor,fill_t);   
      
      //color = float4(fill_t,fill_t,fill_t,1);
   }
   else if(effect < 10)//9
   {
      color = tex2D(_FontSampler, In.TextureUV);
   }
   else if(effect < 11)//10
   {
      //MySoftBorder2
      float d = tex2D(_FontSampler, In.TextureUV).a + _Offset;
      color = float4(0,0,0,0);
      float t = d / _BorderWidth; //[-1:0:1]
      t = abs(1.0 - abs(t)); // [1:0:1:0]
      color = lerp(float4(0,0,0,0), _borderColor, t);
      
      
      
      
      float t2;
      //if(d < -_BorderWidth && d < -_BorderWidth2)
      if(d < -(_BorderWidth))
      {
         t2 = d / _BorderWidth;
         //color = lerp(_borderColor, float4(0,0,0,0), t2);
         color = float4(0,0,0,0);
      }
      
      
      //float t2 = d / _BorderWidth2;
      //color = lerp(_borderColor, float4(0,0,0,0), t2);
      //color = float4(t2,t2,t2,1);
       
      //color = float4(t,t,t,1); 
      //d = abs(d);
      //color = float4(d,d,d,1);
      
   }
   else if(effect < 12) //11
   {
      //BorderTex
      float d = tex2D(_FontSampler, In.TextureUV).a + _Offset;
      color = float4(0,0,0,0);
      float t = d / _BorderWidth; // [-1:0:1]
      t = (t + 1) * 0.5; // [0:1]
      
      if(d > _BorderWidth)
      {
         color = _baseColor;
      }
      else if(d < _BorderWidth)
      {
         color = tex2D(_TexSampler, float2(t,0.5));
      }
      
      if(d < -_BorderWidth)
      {
         color = float4(0,0,0,0);
      }
   
   }
   else if(effect < 13) //12
   {
      //Texture & Gradient
      float4 c = tex2D(_FontSampler, In.TextureUV);
      float4 texc = tex2D(_TexSampler, float2(In.TextureUV2.y,In.TextureUV2.x));
      
      float smoothalpha = smoothstep(_lowThreshold - _smoothing, _lowThreshold + _smoothing, c.a);
      color.a = smoothalpha;
      if(c.a >= _lowThreshold + _smoothing)
      {
         color.a = 1;
      }
      if(c.a <= _lowThreshold - _smoothing)
      {
         color.a = 0;
      }
      color *= _baseColor;
      color *= texc;
   }
   else if(effect < 14) //13
   {
      //Texture & Gradient
      float d = tex2D(_FontSampler, In.TextureUV).a + _Offset;
      color = float4(0,0,0,0);
      float t = d / _BorderWidth; // [-1:0:1]
      
      color = lerp(float4(0,0,0,0), _baseColor, t);
      
      
   }
   else if(effect < 15) //14 smooth 2
   {
      //Texture & Gradient
      float d = tex2D(_FontSampler, In.TextureUV).a;
      
      color = float4(0,0,0,0);
      
      float Thickness = 1.0 - _FontThickness;
      float t = d / Thickness;
      t = t * t;
      t = clamp(t, 0.0, 1.0);
      color = lerp(float4(0,0,0,0)   ,_baseColor , t);
      
      
   }
   else if(effect < 16) //15 Border 2
   {
      float a = tex2D(_FontSampler, In.TextureUV).a;
      color = _baseColor;
      color = float4(0,0,0,0);
      
      //Test
      float outline = smoothstep(_OutlineMinValue - _OutlineMinValue2, _OutlineMinValue + _OutlineMinValue2, a);
      a = smoothstep(1.0 - _FontThickness - _smoothing, 1.0 - _FontThickness + _smoothing, a);
      color.a = a;
      color.rgb = lerp(_borderColor.rgb, color.rgb, outline);
      //Test
      
      
      //Velve
