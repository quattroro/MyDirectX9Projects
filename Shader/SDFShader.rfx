    <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_2_a" ENTRY_POINT="ps_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[

struct VS_OUTPUT 
{
   float4 Position : POSITION0;
   float2 TextureUV : TEXCOORD0;
   float2 TextureUV2 : TEXCOORD1;
};

float4 _baseColor;
float _smoothing;
float _lowThreshold;
float _highThreshold;
float4 _borderColor;
float _borderAlpha;

float effect;
float _Offset;
float _BorderWidth;
float _BorderWidth2;

float _NeonBrightness;
float _NeonPower;
float _ShadowBorderWidth;
float _ShadowDist;
float2 _MainTex_TexelSize;

sampler2D _FontSampler;
sampler2D _TexSampler;

float _FontThickness;
float _FontBorderWidth;

float _OutlineMinValue;
float _OutlineMinValue2;
float _OutlineMaxValue;
float _OutlineMaxValue2;

float4 ps_main(VS_OUTPUT In):COLOR0
{   
   float4 color = _baseColor;
   
   
   //soft
   if(effect<1)//0
   {
      float4 c = tex2D(_FontSampler, In.TextureUV);
      float smoothalpha = smoothstep(_lowThreshold - _smoothing, _lowThreshold + _smoothing, c.a);
      color.a = smoothalpha;
      
      //if(c.a >= _lowThreshold + _smoothing)
      //{
      //   color.a = 1;
      //}
      //if(c.a <= _lowThreshold - _smoothing)
      //{
      //   color.a = 0;
      //}
      
      color *= _baseColor;
   }
   else if(effect<2)//1
   {
      //SharpEdgeDraw
      float alpha = tex2D(_FontSampler, In.TextureUV).a;
      if(alpha >= _lowThreshold)
      {
         color.rgb = _baseColor;
         color.a = 1.0;
      }
      else
      {
         color.rgb = _baseColor;
         color.a = 0.0;
      }   
   }
   else if(effect<3)//2
   {
      //SharpEdgeWithGlowDraw
      float alpha = tex2D(_FontSampler, In.TextureUV).a;
      if(alpha >= _lowThreshold)
      {
         color.rgb = _baseColor;
         color.a = 1.0;
      }
     else if(alpha>0.0f)
     {
        color.rgb = _borderColor;
        color.a = _borderAlpha;
     }
     else
     {
     color.a = 0;
     }
   }
   else if(effect<4)//3
   {
      //BorderDraw
      float alpha = tex2D(_FontSampler, In.TextureUV).a;
      //alpha = smoothstep(0.5 - _smoothing, 0.5 + _smoothing, tex2D(_FontSampler, In.TextureUV).a);
      if(alpha >= _lowThreshold && alpha <= _highThreshold)
      {
         color.rgb = _borderColor;
         color.a = 1;
         //color.a = alpha+ 0.3;
      }
      else if(alpha >= _highThreshold)
      {
         color.rgb = _baseColor;
         color.a = 1;
        //color.a = alpha;
      }
      else
      {
         color.a = 0.0;
      }
   
   }
   else if(effect<5)//4
   {
      //SoftEdge2Draw
      color = _baseColor;
      float alpha = tex2D(_FontSampler, In.TextureUV).a;
     if(alpha < 0.5)
     {
        color.a = smoothstep(0.5 - _smoothing, 0.5 + _smoothing, alpha);
     }
     else
     {
        color.a = smoothstep(0.5 - _smoothing, 0.5 + _smoothing, alpha * 0.95);
     }
   
   }
   else if(effect<6)//5
   {
      //SoftBorder
      float alpha = tex2D(_FontSampler, In.TextureUV).a;
      float d = alpha +  _Offset;
      color = float4(0,0,0,0);
      
      if(d < 0)
      {
         float t = -d / (_BorderWidth);
         t = t * t;
         color = lerp(_borderColor, float4(0,0,0,0), t);
      }
      else if(d < _BorderWidth)
      {
         float t = d / _BorderWidth;
         t = t * t;
         color = lerp(_borderColor, _baseColor, t);
      }
      else if(d > _BorderWidth)
      {
         color = _baseColor;
      }
      if(d < -_BorderWidth)
      {
         color = float4(0,0,0,0);
      }
   
   }
   else if(effect<7)//6
   {
   //float smoothalpha = smoothstep(_lowThreshold - _smoothing, _lowThreshold + _smoothing, c.a);
      //MySoftBorder
      float d = tex2D(_FontSampler, In.TextureUV).a;
      color = float4(0,0,0,0);
      float smoothval = 0;
      
      if(d > _lowThreshold)
      {
         color = _baseColor;
         //color.a = smoothstep(_lowThreshold - _smoothing, _lowThreshold + _smoothing, d);
         
         smoothval = smoothstep(_lowThreshold - _smoothing, _lowThreshold + _smoothing, d);
         if(_BorderWidth<=0)
            color = lerp(float4(0,0,0,0), _baseColor, smoothval);
         else
            color = lerp(_borderColor, _baseColor, smoothval);
      }
      else if(d > _lowThreshold -  _BorderWidth)
      {
         color = _borderColor;
         color.a = smoothstep(_lowThreshold - _BorderWidth - _smoothing, _lowThreshold - _BorderWidth + _smoothing, d);
      }
      //else if(d < _lowThreshold)//inside Glyph liner
      //{
      //   float t =  -d / _BorderWidth;
      //   t = t * t;
      //   color = lerp(_borderColor, _baseColor, t);
      //}
