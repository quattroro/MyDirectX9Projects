      //   t = clamp(t, 0.0, 1.0);
      //   color = lerp(float4(0,0,0,0)   ,_borderColor , t);
      //   //color = float4(t,t,t,1);
      //}
      
      //float t =d / Thickness;
      //t = t * t;
      //color = float4(t,t,t,1);
      //color = lerp(_borderColor   ,_baseColor , t);
      
      //t = d / (Thickness - _FontBorderWidth);
      //t = t * t;
      
      //color = lerp(_borderColor  , color, t);
      
      //float t = d / _BorderWidth; // [-1:0:1]
      //color = lerp(float4(0,0,0,0), _baseColor, t);
      
      
      
      //float t = d / Thickness;
     // t = t * t * t * t;
      //color = lerp(_borderColor   ,_baseColor , t);
      
      
      //t = d / (Thickness - _FontBorderWidth);
      //t = t * t;
      //t = clamp(t, 0.0, 1.0);
      //color = lerp(float4(0,0,0,0)   ,_borderColor , t);
      
      ////OldSoftBorder
      //float alpha = tex2D(_FontSampler, In.TextureUV).a;
      //float d = alpha +  _Offset;
      //color = float4(0,0,0,0);
      //if(d < 0)
      //{
      //   border to Out
      //   float t = -d / (_BorderWidth);
      //   t = t * t;
