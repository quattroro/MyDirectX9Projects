    <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_2_0" ENTRY_POINT="vs_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
float4x4 matWorld;
float4x4 matViewProjection;
//float SinTime;

float uvX;
float uvY;
float uvZoom;
float3 uvparam;


struct VS_INPUT 
{
   float4 Position : POSITION0;
   float2 TextureUV : TEXCOORD0;
   float2 TextureUV2 : TEXCOORD1;
};

struct VS_OUTPUT 
{
   float4 Position : POSITION0;
   float2 TextureUV : TEXCOORD0;
   float2 TextureUV2 : TEXCOORD1;
};

VS_OUTPUT vs_main( VS_INPUT Input )
{
   VS_OUTPUT Output;

   Output.Position = mul( Input.Position, matViewProjection );
   Output.TextureUV = float2(uvparam.x,uvparam.y) + Input.TextureUV /uvparam.z;
   Output.TextureUV2 = Input.TextureUV;
   
   return( Output );
   
}



