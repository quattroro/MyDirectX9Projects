    <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_3_0" ENTRY_POINT="ps_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
//Using Texture URL https://opengameart.org/content/water-caustics-effect-small -> BlendUITextureSampler


struct VS_OUTPUT 
{
   float4 Position : POSITION0;
   float2 TextureUV : TEXCOORD0;
   float2 BlendTextureUV : TEXCOORD1;
};


sampler2D UITextureSampler;
sampler2D BlendUITextureSampler;

float _mtrlDif;
float4 _Color;
//float _OutlineWidth;
//float3 _MainTex_TexelSize;

float _Noise_Amount;
float _radian;
float _tempfloat;
float _xPos;
float _yPos;

float4 _Caustics_ST;
float4 _Caustics_S_ST; 



float4 Generate_Noise(float2 co, float black)
{
     float4 r = frac(sin(dot(co.xy, float2(12.9898, 78.233))) * 43758.5453);
     r.a = saturate(r.a + black);
     return r;
}

float2 hash( float2 p ) 
{
   p = float2( dot(p,float2(127.1,311.7)),
           dot(p,float2(269.5,183.3)) );

   return -1.0 + 2.0*frac(sin(p) * 43758.5453123);
}


float noise_2( in float2 p ) {
    const float K1 = 0.366025404; // (sqrt(3)-1)/2;
    const float K2 = 0.211324865; // (3-sqrt(3))/6;

   float2 i = floor( p + (p.x+p.y) * K1 );

    float2 a = p - i + (i.x+i.y) * K2;
    float2 o = step(a.yx,a.xy);
    float2 b = a - o + K2;
   float2 c = a - 1.0 + 2.0*K2;

    float3 h = max( 0.5-float3(dot(a,a), dot(b,b), dot(c,c) ), 0.0 );

   float3 n = h*h*h*h*float3( dot(a,hash(i+0.0)), dot(b,hash(i+o)), dot(c,hash(i+1.0)));

    return dot( n, float3(70.0,70.0,70.0) );
}

float4 fbm ( in float2 p ) {
    float f = 0.0;
    float2x2 m = { cos(_radian), sin(_radian),-sin(_radian), cos(_radian) };
    
    f  = (0.5000 * noise_2(p)).x; 
    p = mul(p,m);
    f += (0.2500 * noise_2(p)).x; 
    p = mul(p,m);
    f += (0.1250 * noise_2(p)).x; 
    p = mul(p,m);
    f += (0.0625 * noise_2(p)).x; 
    p = mul(p,m);
    f = 0.5 + 0.5 * f;
    float4 result = float4(f,f,f,f);
    return result;
}

float4 rndpixel(float2 p)
{
   float2 ipos = floor(p);
   float2 fpos = frac(p);
   
   float4 color = Generate_Noise(ipos,0);
   color.a = smoothstep(0+_tempfloat,1,color.a);
   //color.w = 1.0f;
   return color;

}

float2 resolution;

float4 bumpMap(float2 uv) {
    float2 s = 1. / resolution.xy;
    float p =  fbm(uv);
    float h1 = fbm(uv + s * float2(1., 0));
    float v1 = fbm(uv + s * float2(0, 1.));

      float2 xy = (p - float2(h1, v1)) * 40;
    return float4(xy + .5, 1.0,1.0);
}
float _splitRGB;
float _CausticsSpeed;
float _Intencity;
float4 _mainColor;

float4 ps_main(VS_OUTPUT In):COLOR0
{   

   float2 originUV = In.TextureUV;
   float4 textureColor = tex2D(UITextureSampler, originUV);// 
   textureColor = _mainColor;
   
   float s = _splitRGB;
   
   float4 noiseColor = bumpMap(originUV* 2* float2(1.0, 0.2)+ float2(_xPos+ _mtrlDif,_yPos+ _mtrlDif ));
   
   //return noiseColor;
   
   
   
   
   float2 uv_first = originUV * _Caustics_ST.xy + _Caustics_ST.zw;
   uv_first.y += _CausticsSpeed * _mtrlDif;
    uv_first += clamp(noiseColor.rg*0.2,-1,1);
   
   float r_first = tex2D(BlendUITextureSampler, uv_first + float2(s,s)).r;
   float g_first = tex2D(BlendUITextureSampler, uv_first + float2(-s,s)).g;
   float b_first = tex2D(BlendUITextureSampler, uv_first + float2(-s,-s)).b;
   float a_first = tex2D(BlendUITextureSampler, uv_first).a;
   
   float4 caustics = float4(r_first, g_first, b_first, a_first);
   
   
     float2 uv_Second = originUV * _Caustics_S_ST.xy + _Caustics_S_ST.zw;
   uv_Second.x += _CausticsSpeed * _mtrlDif;
   uv_Second += clamp(noiseColor.rg*0.2,-1,1);
   
   
   float r_Second = tex2D(BlendUITextureSampler, uv_Second + float2(s,s)).r;
   float g_Second = tex2D(BlendUITextureSampler, uv_Second + float2(s,-s)).g;
   float b_Second = tex2D(BlendUITextureSampler, uv_Second + float2(-s,-s)).b;
   float a_Second = tex2D(BlendUITextureSampler, uv_Second).a;
   
   float4 caustics2 = float4(r_Second, g_Second, b_Second, a_Second);
   
   // float4 noiseColor = bumpMap(originUV* 3* float2(1.0, 0.2)+ float2(_xPos,_yPos+ _mtrlDif ));
     //textureColor = tex2D(UITextureSampler, originUV + clamp(noiseColor.rg*0.5,-1,1));// 
   
   
   textureColor += min(caustics, caustics2) * _Intencity;
   
   //textureColor = min(caustics, caustics2);
   //textureColor = caustics2;
   //textureColor.xyz =clamp((float3(1,1,1) - min(caustics, caustics2).xyz)* _Intencity,0,1);
   textureColor *= _Color;
     
   return( noiseColor );
   
}



]]></RmShaderCode>
