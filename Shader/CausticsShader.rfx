   float2 TextureUV2 : TEXCOORD1;
};

VS_OUTPUT vs_main( VS_INPUT Input )
{
   VS_OUTPUT Output;

   Output.Position = mul( Input.Position, matViewProjection );
   Output.TextureUV = Input.TextureUV;
   Output.TextureUV2 = Input.TextureUV2;
   
   
   return( Output );
   
}



