    <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_3_0" ENTRY_POINT="ps_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[

struct VS_OUTPUT 
{
   float4 Position : POSITION0;
   float2 TexCoord : TEXCOORD0;
};


float fTime;
float radian;
float2 resolution;
float xPos;
float yPos;
float4 leakColor;
float4 leakColor2;


sampler2D UITextureSampler;
sampler2D mainTexSampler;
sampler2D Texture0;

float4 Generate_Noise(float2 co, float black)
{
     float4 r = frac(sin(dot(co.xy, float2(12.9898, 78.233))) * 43758.5453);
     r.a = saturate(r.a + black);
     return r;
}

float2 hash( float2 p ) 
{
   p = float2( dot(p,float2(127.1,311.7)),
           dot(p,float2(269.5,183.3)) );

   return -1.0 + 2.0*frac(sin(p) * 43758.5453123);
}


float noise_2( in float2 p ) {
    const float K1 = 0.366025404; // (sqrt(3)-1)/2;
    const float K2 = 0.211324865; // (3-sqrt(3))/6;

   float2 i = floor( p + (p.x+p.y) * K1 );

    float2 a = p - i + (i.x+i.y) * K2;
    float2 o = step(a.yx,a.xy);
    float2 b = a - o + K2;
   float2 c = a - 1.0 + 2.0*K2;

    float3 h = max( 0.5-float3(dot(a,a), dot(b,b), dot(c,c) ), 0.0 );

   float3 n = h*h*h*h*float3( dot(a,hash(i+0.0)), dot(b,hash(i+o)), dot(c,hash(i+1.0)));

    return dot( n, float3(70.0,70.0,70.0) );
}

float4 fbm ( in float2 p ) {
    float f = 0.0;
    float2x2 m = { cos(radian), sin(radian),-sin(radian), cos(radian) };
    
    f  = (0.5000 * noise_2(p)).x; 
    p = mul(p,m);
    f += (0.2500 * noise_2(p)).x; 
    p = mul(p,m);
    f += (0.1250 * noise_2(p)).x; 
    p = mul(p,m);
    f += (0.0625 * noise_2(p)).x; 
    p = mul(p,m);
    f = 0.5 + 0.5 * f;
    float4 result = float4(f,f,f,f);
    return result;
}

float4 bumpMap(float2 uv) {
    float2 s = 1. / resolution.xy;
    float p =  fbm(uv);
    float h1 = fbm(uv + s * float2(1., 0));
    float v1 = fbm(uv + s * float2(0, 1.));

      float2 xy = (p - float2(h1, v1)) * 20;
    return float4(xy + .5, 1.0,1.0);
}


//Flare
const float pi = 3.14159;

float ring(float2 p, float2 pos, float r1, float r2)
{
   float2 a = normalize(p - pos);
   return length((pos + a * r1) - p) - r2;
}

float2 rot(float2 a, float c)
{
   float l = length(a);
   a /= l;
   float ang = (a.y < 0.) ? 2.0 * pi - acos(a.x) : acos(a.x);
   ang += c;
   return float2(l * cos(ang), l * sin(ang));
}

float3 lensflare(float2 uv, float2 mouse)
{    
    float3 col = float3(0.0, 0.0, 0.0);
    float dist = length(mouse - uv); // UV to mouse Vec Lenth
    float2 pos = normalize(uv - mouse); 
   
    dist = length(mouse - uv);
    float2 mouse2 = normalize(mouse);
    float ang = (mouse2.y < 0.0) ? 2.0 * pi - acos(mouse2.x) : acos(mouse2.x);
    col += sin(tex2D(Texture0, rot(pos*0.06, -ang)).x) * cos(tex2D(Texture0, rot(pos*0.04, -ang)).x) * 0.2;
    //col += sin(hash(rot(pos*0.06, -ang)).x) * cos(hash(rot(pos*0.04, -ang)).x) * 0.2;
    
    col += clamp(float3(2.0, 2.0, 2.0) - clamp(dist * 1.5, 0.0, 1.0) * 2.1, 0.0, 1.0) * float3(0.9, 0.7, 0.2);
    dist = length(-mouse - uv) - 0.05;
    col += (2.0 - length(mouse - uv) - 0.04) * float3(0.9, 0.6, 0.2);
    float rin = ring(uv, -mouse, 0.2, 0.05);
    col += clamp(3.0-rin*2.0, -0.4, 0.4);
    
    col += clamp(float3(1.0, 1.0, 1.0)-clamp(dist*dist*dist+dist*1.5,0.0,1.0)*2.1,0.0,1.0)*(float3(length(mouse-uv), 1.0-length(-mouse-uv), 0.2));
    dist = length(-mouse*0.2-uv)-0.001;
    
    col += clamp(float3(1.0,1.0,1.0)-clamp(dist*1.5,0.0,1.0)*4.0,0.0,1.0)*float3(0.9,0.6,0.3);
    dist = length(-mouse*0.6-uv)-0.001;
    
    col += clamp(float3(3.0,3.0,3.0)-clamp(dist*2.5,0.0,1.0)*4.0,0.0,1.0)*float3(0.9,0.6,0.3);
    dist = length(-mouse*0.5-uv)-0.4;
    
    col += clamp(float3(3.0, 3.0, 3.0)-dist*dist*dist+dist*1.5,0.0,1.0)*float3(0.9,0.6,0.2);
    col += clamp(float3(0.9, 0.7, 0.2)-ring(uv, mouse, 0.1,0.04)*2.0-0.1,0.0,1.0);
    col += clamp(1.0 - ring(uv, -mouse * 0.1, 0.2, 0.05) * 2.0, 0.0, 1.0) * 0.8 * float3(0.9, 0.7, 0.7);
    
    col /= 2.0;
    col = clamp(col, 0.0, 1.0);
    return col;
}
/////



float4 ps_main(VS_OUTPUT IN) : COLOR0
{  
   float2 oriUV = IN.TexCoord;
   float4 result = float4(0,0,0,0);
   
   //float2 uv = oriUV/resolution.xy;
   //float4 normal = bumpMap(uv * float2(0.2, 0.2) + float2(0.1, 0.0) * fTime);
   //float2 displacement = clamp((normal.xy - .5) * 1.2, -1., 1.);
   
   //not distortion
   float4 noiseTex = bumpMap(oriUV * 1.0 * float2(0.2, 0.2)  + float2(xPos - fTime / 7, yPos));
   
   //use distortion
   //float4 noiseTex = bumpMap()
   
   //- fTime / 7
   float4 color = tex2D(UITextureSampler, float2(noiseTex.g, 0.5));
   float4 mainTex = tex2D(mainTexSampler, IN.TexCoord);
   //float4(noiseTex.g, noiseTex.g, noiseTex.g, 1)
   float4 leak = float4(noiseTex.g, noiseTex.g, noiseTex.g, 1) * leakColor;
   float4 leak2 = pow(float4(noiseTex.r, noiseTex.r, noiseTex.r, 1), 5);
   float4 leak3 = abs(1.0 - leak2);
   leak3.a = 1;
   float4 leak4 = ((float4(noiseTex.g, noiseTex.g, noiseTex.g, 1) - 0.4) * leakColor) * 1.3;
   leak4.a = 1;
   float4 leak5 = ((leak2)) * 2.3 * leakColor;
   leak5.a = 0.5;
   
   float4 leak6 = (pow(float4(noiseTex.r, noiseTex.r, noiseTex.r, 1), 10)) * 7.3 * leakColor2;
   
   float3 lenColor = lensflare(oriUV, float2(0.1 + 4.0 * sin(fTime/ 7),0.3));
   
   
   float3 R;
   float3 G;
   float3 B;
   
   //mainTex * leak
   //mainTex + leak4
   //mainTex + max(leak5, leak6)
   //mainTex + lerp(leak5, leak6, 0.5)
   //result = mainTex + lerp(leak5, leak6, 0.5);
   //result = mainTex;
   //result.rgb += lenColor;
   return(float4(lenColor,1.0));
}



]]></RmShaderCode>
