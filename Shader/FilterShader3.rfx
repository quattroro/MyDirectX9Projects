    <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_3_0" ENTRY_POINT="ps_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[

struct VS_OUTPUT 
{
   float4 Position : POSITION0;
   float2 TexCoord : TEXCOORD0;
};


float fTime;
float radian;
float2 resolution;


sampler2D UITextureSampler;
sampler2D mainTexSampler;
sampler2D woodTexSampler;

float4 vignette(float2 uv, float time) 
{
    uv *=  1.0 - uv.yx;   
    float vig = uv.x *uv.y * 15.0;
    float t = sin(time * 23.) * cos(time * 8. + .5);
    vig = pow(vig, 0.4 + t * .05);
    return float4(vig, vig, vig, vig);
}

float easeIn(float t0, float t1, float t) 
{
   return 2.0 * smoothstep(t0, 2.0 * t1-t0, t);
}

float4 blackAndWhite(float4 color) 
{
    float dotval = dot(color.xyz, float3(.299, .587, .114));
    return float4(dotval, dotval, dotval, dotval);
}


float filmDirt(float2 pp, float time) //-1 ~ 1
{
   float aaRad = 0.1;
   float2 nseLookup2 = pp + float2(0.5,0.9) + time * 100.0;
   float3 nse2 =
      tex2D(woodTexSampler, 0.1*nseLookup2.xy).xyz +
      tex2D(woodTexSampler, 0.01*nseLookup2.xy).xyz +
      tex2D(woodTexSampler, 0.004*nseLookup2.xy+0.4).xyz;
      
      //return nse2.x;
      
   float thresh = 0.6;
   float mul1 = smoothstep(thresh-aaRad, thresh+aaRad, nse2.x);
   float mul2 = smoothstep(thresh-aaRad, thresh+aaRad, nse2.y);
   float mul3 = smoothstep(thresh-aaRad, thresh+aaRad, nse2.z);
   
   //return mul1;
   
   float seed = tex2D(woodTexSampler, float2(time * 0.35, time)).x;
   
   float result = clamp(0.,1.,seed+.7) + .3 * smoothstep(0.0, 24.0, time);
   
   result += .06*easeIn(19.2,19.4,time);

   float band = .05;
   if( 0.3 < seed && .3+band > seed )
      return mul1 * result;
   if( 0.6 < seed && .6+band > seed )
      return mul2 * result;
   if( 0.9 < seed && .9+band > seed )
      return mul3 * result;
   return result;
}

float4 jumpCut(float seqTime) 
{
   float toffset = 0.;
   float3 camoffset = float3(0.0, 0.0, 0.0);
   
   float jct = seqTime;
   float jct1 = 7.7;
   float jct2 = 8.2;
   float jc1 = step( jct1, jct );
   float jc2 = step( jct2, jct );
   
   camoffset += float3(0.8, 0.0, 0.0) * jc1;
   camoffset += float3(-0.8, 0.0, 0.0) * jc2;
   
   toffset += 0.8 * jc1;
   toffset -= (jc2-jc1)*(jct-jct1);
   toffset -= 0.9 * jc2;
   
   return float4(camoffset, toffset);
}

float limitFPS(float time, float fps) 
{
    time = fmod(time, 24.0);
    return float(int(time * fps)) / fps;
}

float2 moveImage(float2 uv, float time) 
{
    uv.x += .002 * (cos(time * 3.) * sin(time * 12. + .25));
    uv.y += .002 * (sin(time * 1. + .5) * cos(time * 15. + .25));
    return uv;
}

float hash(float n) {
    return frac(cos(n*89.42)*343.42);
}

float2 hash2(float2 n) {
    return float2(hash(n.x*23.62-300.0+n.y*34.35),hash(n.x*45.13+256.0+n.y*38.89)); 
}

float worley(float2 c, float time) {
    float dis = 1.0;
    for(int x = -1; x <= 1; x++)
        for(int y = -1; y <= 1; y++){
            float2 p = floor(c)+float2(x,y);
            float2 a = hash2(p) * time;
            float2 rnd = 0.5+sin(a)*0.5;
            float d = length(rnd+float2(x,y)-frac(c));
            dis = min(dis, d);
        }
    return dis;
}

float worley2(float2 c, float time) {
    float w = 0.0;
    float a = 0.5;
    for (int i = 0; i<2; i++) {
        w += worley(c, time)*a;
        c*=2.0;
        time*=2.0;
        a*=0.5;
    }
    return w;
}

float4 film(float2 uv) {
    float wm = 0.8 + (/*filmWidth*/1.3-1.0)/4.0;
    float ww = (/*filmWidth*/1.7-1.0)*0.1;
    float ax = abs(uv.x);
    if (ax > /*filmWidth*/2.0/2.0 || (ax > wm-ww && ax < wm+ww && fmod(floor(abs(uv.y*12.0 - fTime * 20)),2.0) == 1.0))
       return float4(float3(1.0,1.0,1.0), 1.0);

    uv.x *= 2000.10 ;
    uv.y *= 5.;
    //float dis = worley2(uv/64.0, + fTime*50.0);
    float dis = worley2(uv/64.0, + fTime*50.0);
    float3 c = lerp(float3(1.0,1.0,1.0), float3(-30.6,-30.6,-30.6), dis);
    return float4(clamp(1.0-c*c,0.0,1.0), 1.0);
}

float4 film2(float2 uv) {
    float wm = 0.8 + (/*filmWidth*/1.3-1.0)/4.0;
    float ww = (/*filmWidth*/1.7-1.0)*0.1;
    float ax = abs(uv.x);
    if (ax > /*filmWidth*/2.0/2.0 || (ax > wm-ww && ax < wm+ww && fmod(floor(abs(uv.y*12.0 + fTime)),2.0) == 1.0))
       return float4(float3(0.58,0.58,0.58), 1.0);

    uv.x *= 2000.10 ;
    uv.y *= 5.;
    //float dis = worley2(uv/64.0, + fTime*50.0);
    float dis = worley2(uv/64.0, + fTime*50.0);
    float3 c = lerp(float3(1.0,1.0,1.0), float3(-30.6,-30.6,-30.6), dis);
    return float4(clamp(1.0-c*c,0.0,1.0), 1.0);
}

float4 ps_main(VS_OUTPUT IN) : COLOR0
{  
   float2 oriUV = IN.TexCoord;
   float4 result = float4(1,1,1,1);
   
   float2 uv = oriUV;
   uv *= 1.2;
   uv -= 0.1;
   float2 qq = -1.0 + 2.0*uv; // 0~1 -> -1~1
   //qq.x *= resolution.x / resolution.y;
    
   float time = limitFPS(fTime, 12.0);

   float4 jumpCutData = jumpCut(time);
   float filmdirt = filmDirt(qq, time + jumpCutData.w);
   //float filmdirt = 1.0;
   float4 dirt = float4(filmdirt, filmdirt, filmdirt, filmdirt);     
   float4 image = tex2D(mainTexSampler, moveImage(uv, fTime));   
   float4 vig = vignette(uv, time);
    
   result = image * dirt * vig;
   result = blackAndWhite(result);
   result = result + film(float2(-1.0 + 2.0*oriUV.x, oriUV.y));
   //result = result * film2(float2(-1.0 + 2.0*oriUV.x, oriUV.y));
   
   return result;
}



]]></RmShaderCode>
