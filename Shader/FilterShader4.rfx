    <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_3_0" ENTRY_POINT="ps_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[

struct VS_OUTPUT 
{
   float4 Position : POSITION0;
   float2 TexCoord : TEXCOORD0;
};

//#define DEBUG

float fTime;
float radian;
float2 iResolution;


sampler2D UITextureSampler;
sampler2D mainTexSampler;
sampler2D woodTexSampler;


float3 N13(float p) {
   //  from DAVE HOSKINS
   float3 p3 = frac(float3(p, p, p) * float3(.1031,.11369,.13787));
   p3 += dot(p3, p3.yzx + 19.19);
   return frac(float3((p3.x + p3.y)*p3.z, (p3.x+p3.z)*p3.y, (p3.y+p3.z)*p3.x));
}

float4 N14(float t) {
   return frac(sin(t*float4(123., 1024., 1456., 264.))*float4(6547., 345., 8799., 1564.));
}
float N(float t) {
    return frac(sin(t*12345.564)*7658.76);
}

float Saw(float b, float t) {
   return smoothstep(0., b, t)*smoothstep(1., b, t);
}


float2 Drops(float2 uv, float t) {
    
    float2 UV = uv;
    
    // DEFINE GRID
    uv.y += t * 0.8;
    float2 a = float2(6.0, 1.0);
    float2 grid = a * 2.0;
    float2 id = floor(uv * grid);
    
    // RANDOM SHIFT Y
    float colShift = N(id.x); 
    uv.y += colShift;
    
    // DEFINE SPACES
    id = floor(uv * grid);
    float3 n = N13(id.x * 35.2 + id.y * 2376.1);
    float2 st = frac(uv * grid) - float2(.5, 0);
    
    // POSITION DROPS
    //clamp(2*x,0,2)+clamp(1-x*.5, -1.5, .5)+1.5-2
    float x = n.x - 0.5;
    
    float y = UV.y * 20.0;
    
    float distort = sin(y + sin(y));
    x += distort*(0.5 - abs(x)) * (n.z - 0.5);
    x *= 0.7;
    float ti = frac(t+n.z);
    y = (Saw(0.85, ti)-0.5)*0.9+0.5;
    float2 p = float2(x, y);
    
    // DROPS
    float d = length((st-p)*a.yx);
    
    float dSize = 0.2; 
    
    float Drop = smoothstep(dSize, .0, d);
    
    
    float r = sqrt(smoothstep(1., y, st.y));
    float cd = abs(st.x-x);
    
    // TRAILS
    float trail = smoothstep((dSize*.5+.03)*r, (dSize*.5-.05)*r, cd);
    float trailFront = smoothstep(-.02, .02, st.y-y);
    trail *= trailFront;
    
    
    // DROPLETS
    y = UV.y;
    y += N(id.x);
    float trail2 = smoothstep(dSize*r, .0, cd);
    float droplets = max(0., (sin(y*(1.-y)*120.)-st.y))*trail2*trailFront*n.z;
    y = frac(y*10.)+(st.y-.5);
    float dd = length(st-float2(x, y));
    droplets = smoothstep(dSize*N(id.x), 0., dd);
    float m = Drop+droplets*r*trailFront;
    #ifdef DEBUG
    m += st.x>a.y*.45 || st.y>a.x*.165 ? 1.2 : 0.; //DEBUG SPACES
    #endif
    
    
    return float2(m, trail);
}

float StaticDrops(float2 uv, float t) {
   uv *= 30.;
    
    float2 id = floor(uv);
    uv = frac(uv)-.5;
    float3 n = N13(id.x*107.45+id.y*3543.654);
    float2 p = (n.xy-.5)*0.5;
    float d = length(uv-p);
    
    float fade = Saw(.025, frac(t+n.z));
    float c = smoothstep(0.2, 0., d)*frac(n.z*10.)*fade;

    return c;
}

float2 Rain(float2 uv, float t) {
    float s = StaticDrops(uv, t); 
    float2 r1 = Drops(uv, t);
    float2 r2 = Drops(uv * 1.8, t);
    
    #ifdef DEBUG
    float c = r1.x;
    #else
    float c = s + r1.x + r2.x;
    #endif
    
    c = smoothstep(0.3, 1.0, c);
    
    #ifdef DEBUG
    return float2(c, r1.y);
    #else
    return float2(c, max(r1.y, r2.y));
    #endif
}

float4 ps_main(VS_OUTPUT IN) : COLOR0
{  
   float2 oriUV = IN.TexCoord;
   float4 result = float4(1,1,1,1);
   
   //float2 uv = (oriUV.xy-.5*iResolution.xy) / iResolution.y;
   //float2 UV = oriUV.xy/iResolution.xy;
   
   float2 uv = float2(oriUV.x,-oriUV.y);
   float2 UV = oriUV;
   
   float T = fTime;
   
   float t = T*.2;
    
   float rainAmount = 0.8;
    

    
   UV = (UV-.5)*(.9)+.5;
    
   float2 c = Rain(uv, t);

   float2 e = float2(.001, 0.); //pixel offset
   float cx = Rain(uv+e, t).x;
   float cy = Rain(uv+e.yx, t).x;
   float2 n = float2(cx-c.x, cy-c.x); //normals
   
   // BLUR derived from existical https://www.shadertoy.com/view/Xltfzj
   float Pi = 6.28318530718; // Pi*2
    
   // GAUSSIAN BLUR SETTINGS {{{
   float Directions = 32.0; // BLUR DIRECTIONS (Default 16.0 - More is better but slower)
   float Quality = 8.0; // BLUR QUALITY (Default 4.0 - More is better but slower)
   float Size = 32.0; // BLUR SIZE (Radius)
   // GAUSSIAN BLUR SETTINGS }}}

   float2 Radius = Size/iResolution.xy;

   float3 col = tex2D(mainTexSampler, UV).rgb;
   // Blur calculations
   for( float d=0.0; d<Pi; d+=Pi/Directions)
   {
       for(float i=1.0/Quality; i<=1.0; i+=1.0/Quality)
       {
           #ifdef DEBUG
           float3 tex = tex2D( mainTexSampler, UV+c+float2(cos(d),sin(d))*Radius*i).rgb;
           #else
           float3 tex = tex2D( mainTexSampler, UV+n+float2(cos(d),sin(d))*Radius*i).rgb;
           #endif

           col += tex;            
       }
   }

   col /= Quality * Directions - 0.0;

   float3 tex = tex2D( mainTexSampler, UV+n).rgb;
   c.y = clamp(c.y, 0.0, 1.);

   col -= c.y;
   col += c.y*(tex+.6);
   
   result = float4(col, 1.0);
   
   return result;
}



]]></RmShaderCode>
